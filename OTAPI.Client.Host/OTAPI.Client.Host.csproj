<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net5.0</TargetFramework>
		<CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
		<RuntimeIdentifiers>win7-x64;osx.10.11-x64;ubuntu.16.04-x64</RuntimeIdentifiers>
		<GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
		<PlatformTarget>AnyCPU</PlatformTarget>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<AssemblyName>Terraria</AssemblyName>
    <PlatformTarget>x64</PlatformTarget>
	</PropertyGroup>
	<Import Condition="Exists('..\OTAPI.Client.Installer\bin\Debug\net5.0\AutoGenerated.target')" Project="..\OTAPI.Client.Installer\bin\Debug\net5.0\AutoGenerated.target" />
	<Import Project="PublishAllRids.targets" />
	<ItemGroup>
		<Reference Include="OTAPI">
			<HintPath>..\OTAPI.Client.Installer\bin\Debug\net5.0\OTAPI.exe</HintPath>
		</Reference>
		<Reference Include="OTAPI.Runtime">
			<HintPath>..\OTAPI.Client.Installer\bin\Debug\net5.0\OTAPI.Runtime.dll</HintPath>
		</Reference>
		<Reference Include="SteelSeriesEngineWrapper">
			<HintPath>..\OTAPI.Client.Installer\bin\Debug\net5.0\SteelSeriesEngineWrapper.dll</HintPath>
		</Reference>
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="ImGui.NET" Version="1.78.0" />
		<PackageReference Include="ModFramework" Version="1.0.0-alpha4" />
		<PackageReference Include="ModFramework.Modules.ClearScript" Version="1.0.0-alpha4" />
		<PackageReference Include="ModFramework.Modules.CSharp" Version="1.0.0-alpha4" />
		<PackageReference Include="ModFramework.Modules.Lua" Version="1.0.0-alpha4" />
		<PackageReference Include="NLua" Version="1.5.9" />
		<PackageReference Include="Microsoft.Extensions.Http" Version="5.0.0" />
		<PackageReference Include="System.Security.Permissions" Version="5.0.0" />
		<PackageReference Include="System.Runtime.InteropServices" Version="4.3.0" />
		<PackageReference Include="Microsoft.ClearScript" Version="7.1.4" />
		<PackageReference Include="Microsoft.ClearScript.V8.Native.osx-x64" Version="7.1.4" />
		<PackageReference Include="Microsoft.ClearScript.linux-x64" Version="7.1.4" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\FNA\FNA.Core.csproj" />
	</ItemGroup>
	<ItemGroup>
		<None Remove="System.Reflection.Emit" />
		<None Remove="Microsoft.ClearScript" />
		<None Remove="Microsoft.ClearScript.V8.Native.osx-x64" />
		<None Remove="Microsoft.ClearScript.linux-x64" />
	</ItemGroup>
	<ItemGroup>
	  <Compile Update="App.axaml.cs">
	    <SubType>Code</SubType>
	    <DependentUpon>%(Filename)</DependentUpon>
	  </Compile>
	  <Compile Update="MainWindow.axaml.cs">
	    <SubType>Code</SubType>
	    <DependentUpon>%(Filename)</DependentUpon>
	  </Compile>
	</ItemGroup>
	<ItemGroup>
	  <None Update="App.axaml">
	    <SubType>Designer</SubType>
	  </None>
	  <None Update="MainWindow.axaml">
	    <SubType>Designer</SubType>
	  </None>
	</ItemGroup>
	<Target Name="CleanAll" AfterTargets="Clean">
		<RemoveDir Directories="$(OUTDIR)" />
	</Target>
</Project>